{
  "name": "vscode-vcard",
  "displayName": "vscode-vcard",
  "description": "vCard Syntax Highlighting for Visual Studio Code",
  "version": "0.2.2",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "vcf",
      "aliases": ["vCard", "vcard", "vcf"],
      "extensions": [".vcf",".vcard"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vcf",
      "scopeName": "source.vcf",
      "path": "./syntaxes/vcf.tmLanguage.json"
    }]
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}