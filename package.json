{
    "name": "vscode-vcard",
    "displayName": "vscode-vcard",
    "description": "vCard Syntax Highlighting for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "vcf",
            "aliases": ["vCard", "vcf"],
            "extensions": [".vcf",".vcard"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vcf",
            "scopeName": "source.vcf",
            "path": "./syntaxes/vcf.tmLanguage.json"
        }]
    }
}