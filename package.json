{
  "name": "vcard",
  "displayName": "vCard",
  "description": "vCard Syntax Highlighting for Visual Studio Code",
  "publisher": "cstrachan",
  "author": {
    "name": "Courtney Strachan",
    "email": "courtney.strachan@gmail.com",
    "url": "https://strachan.dev/"
  },
  "license": "MIT",
  "version": "1.0.0",
  "private": false,
  "icon": "images/vcard-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/cstrachan88/vscode-vcard.git"
  },
  "bugs": {
    "url": "https://github.com/cstrachan88/vscode-vcard/issues"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "vcard",
      "aliases": ["vCard", "vcard", "vcf"],
      "extensions": [".vcard",".vcf"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vcard",
      "scopeName": "source.vcard",
      "path": "./syntaxes/vcard.tmLanguage.json"
    }]
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}